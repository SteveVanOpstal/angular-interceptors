{
  "name": "angular-interceptors",
  "version": "2.0.4",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "package": "ng-packagr -p package.json",
    "format": "clang-format --glob=\"@(src)/**/*.@(ts|js)\" -i && clang-format --glob=\"*.@(ts|js)\" -i",
    "lint": "tslint -p . -c tslint.json \"src/**/*.ts\" \"public_api.ts\"",
    "release": "standard-version --release-as patch",
    "postrelease": "git push --follow-tags origin master && yarn package && conventional-github-releaser -p angular"
  },
  "devDependencies": {
    "@angular/common": ">=5.2.3 <6.0.0",
    "@angular/compiler": ">=5.2.3 <6.0.0",
    "@angular/compiler-cli": ">=5.2.3 <6.0.0",
    "@angular/core": ">=5.2.3 <6.0.0",
    "@angular/language-service": ">=5.2.3 <6.0.0",
    "@angular/platform-browser": ">=5.2.3 <6.0.0",
    "@angular/platform-browser-dynamic": ">=5.2.3 <6.0.0",
    "clang-format": "^1.1.0",
    "codelyzer": "^4.1.0",
    "conventional-github-releaser": "^2.0.0",
    "core-js": "2.x",
    "ng-packagr": "1.7.0",
    "rxjs": "5.x",
    "standard-version": "^4.3.0",
    "tsickle": "^0.26",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": ">=2.4.2 <2.7",
    "zone.js": "0.x"
  },
  "ngPackage": {
    "dest": "lib",
    "lib": {
      "entryFile": "public_api.ts"
    }
  }
}

{
  "name": "angular-interceptors",
  "version": "1.0.1",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "package": "ng-packagr -p package.json",
    "format": "clang-format --glob=\"@(src)/**/*.@(ts|js)\" -i && clang-format --glob=\"*.@(ts|js)\" -i",
    "lint": "tslint -p . -c tslint.json \"src/**/*.ts\" \"public_api.ts\"",
    "release": "yarn package && standard-version --release-as patch",
    "postrelease": "git push --follow-tags origin master && conventional-github-releaser -p angular"
  },
  "dependencies": {
    "@angular/common": "~5.2.0",
    "@angular/compiler": "~5.2.0",
    "@angular/core": "~5.2.0",
    "@angular/platform-browser": "~5.2.0",
    "@angular/platform-browser-dynamic": "~5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.5",
    "typescript": "^2.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~5.2.0",
    "@angular/language-service": "~5.2.0",
    "clang-format": "^1.1.0",
    "codelyzer": "4.0.2",
    "conventional-github-releaser": "^2.0.0",
    "ng-packagr": "2.0.0-rc.9",
    "standard-version": "^4.3.0",
    "tsickle": "^0.26",
    "tslint": "^5.9.1"
  },
  "ngPackage": {
    "dest": "lib",
    "lib": {
      "entryFile": "public_api.ts"
    }
  }
}
